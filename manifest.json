{
  "manifest_version": 2,
  "name": "Media Content",
  "description": "Extension for parsing media content from web pages such as images, videos, music and so on",
  "version": "1.0",
  "browser_action": {
    "default_title": "Media Parser",
    "default_popup": "./extension/index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts/index.js"]
    }
  ],
  "background": {
    "scripts": ["background/background.js"]
  }
}